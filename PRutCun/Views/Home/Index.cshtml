@model PRutCun.Models.Mapas
@{
    ViewData["Title"] = "Rutcun";
    Layout = "_LayoutDashboard";
}

<div class="contenedor">


    <div>
        <div class="contenerdor__combos">
            <p class="TextSel"> Rutas</p>
            <form asp-action="Mapa" method="post">
                <select class="Combo1" asp-for="Fktransportes" asp-items="ViewBag.Transporte">

                    <option value=""></option>
                </select>
                <button id="btnEnviar" type="submit">Enviar</button>
            </form>

        </div>

    </div>

    <p class="TextMap"> Mapa con Ruta</p>
    <iframe class="mapa" src="@Html.DisplayFor(x=>x.MapaUrl)" width="640" height="480"></iframe>


</div>